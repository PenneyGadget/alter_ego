<%= form_for @team do |f| %>
  <%= f.label :name, "Team Name" %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :password, "Team Passcode" %>
  <%= f.password_field :password %>
  <br>
  <%= f.label :password_confirmation, "Team passcode confirmation" %>
  <%= f.password_field :password_confirmation %>
  <br>
  <%= f.submit "Create Team" %>
<% end %>
